{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Api": {
    "Explorer": {
      "GroupNameFormat": "'v'VVV",
      "ReportApiVersions ": true
    },
    "Versioning": {
      "SubstituteApiVersionInUrl": true
    }
  },
  "AllowedHosts": "*"
}
